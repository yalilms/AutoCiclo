<project>
    <shortName>sample</shortName>
    <fullName>AutoCiclo</fullName>
    <version>1.0</version>
    <componentList>
        <component name="default">
            <description>Default Component</description>
            <canBeEdited>1</canBeEdited>
            <selected>1</selected>
            <show>1</show>
            <desktopShortcutList>
                <shortcut>
                    <comment>Gestión de vehículos y piezas</comment>
                    <exec></exec>
                    <icon></icon>
                    <name>AutoCiclo</name>
                    <path></path>
                    <platforms>all</platforms>
                    <runAsAdmin>0</runAsAdmin>
                    <runInTerminal>0</runInTerminal>
                    <windowsExec>${installdir}/CarpetaInstalacion/run.bat
</windowsExec>
                    <windowsExecArgs></windowsExecArgs>
                    <windowsIcon>${installdir}/CarpetaInstalacion/iconos/autociclo.ico
</windowsIcon>
                    <windowsPath>${installdir}/CarpetaInstalacion
</windowsPath>
                </shortcut>
            </desktopShortcutList>
            <folderList>
                <folder destination="${installdir}" name="programfiles">
                    <description>Program Files</description>
                    <platforms>all</platforms>
                    <shortcutList>
                        <shortcut>
                            <comment>Uninstall</comment>
                            <exec>${installdir}/${uninstallerName}</exec>
                            <icon></icon>
                            <name>Uninstall ${product_fullname}</name>
                            <path>${installdir}</path>
                            <platforms>all</platforms>
                            <runAsAdmin>0</runAsAdmin>
                            <runInTerminal>0</runInTerminal>
                            <windowsExec>${installdir}/${uninstallerName}.exe</windowsExec>
                            <windowsExecArgs></windowsExecArgs>
                            <windowsIcon></windowsIcon>
                            <windowsPath>${installdir}</windowsPath>
                        </shortcut>
                    </shortcutList>
                </folder>
                <folder destination="${installdir}" name="programfileslinux">
                    <description>Program Files</description>
                    <platforms>linux</platforms>
                </folder>
                <folder destination="${installdir}" name="programfileslinux64">
                    <description>Program Files</description>
                    <platforms>linux-x64</platforms>
                </folder>
                <folder destination="${installdir}" name="programfileswindows">
                    <description>Program Files</description>
                    <platforms>windows</platforms>
                </folder>
                <folder destination="${installdir}" name="programfileswindows64">
                    <description>Program Files</description>
                    <platforms>windows-x64</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>/home/yalilms/Documentos/2ºDAM/Desarrollo_interfaces/AutoCiclo/Instaladores/CarpetaInstalacion</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
                <folder destination="${installdir}" name="programfileswindowsarm64">
                    <description>Program Files</description>
                    <platforms>windows-arm64</platforms>
                </folder>
                <folder destination="${installdir}" name="programfilesosx">
                    <description>Program Files</description>
                    <platforms>osx</platforms>
                </folder>
            </folderList>
            <startMenuShortcutList>
                <startMenuShortcut>
                    <comment>Uninstall ${product_fullname}</comment>
                    <name>Uninstall ${product_fullname}</name>
                    <runAsAdmin>0</runAsAdmin>
                    <runInTerminal>0</runInTerminal>
                    <windowsExec>${installdir}/${uninstallerName}.exe</windowsExec>
                    <windowsExecArgs></windowsExecArgs>
                    <windowsIcon></windowsIcon>
                    <windowsPath>${installdir}/</windowsPath>
                </startMenuShortcut>
            </startMenuShortcutList>
        </component>
    </componentList>
    <preInstallationActionList>
        <autodetectJava>
            <customErrorMessage>AutoCiclo requiere Java 21 o superior. Por favor, instale Java antes de continuar.
</customErrorMessage>
            <explanation>Verificando instalacion de Java...
</explanation>
            <progressText>Buscando Java en el sistema
</progressText>
            <promptUser>0</promptUser>
        </autodetectJava>
        <launchBrowser>
            <abortOnError>0</abortOnError>
            <explanation>Abriendo pagina de descarga de Java...</explanation>
            <run>${java_autodetected}!=1
</run>
            <showMessageOnError>0</showMessageOnError>
            <url>https://www.oracle.com/java/technologies/downloads/#java21
</url>
        </launchBrowser>
    </preInstallationActionList>
    <enableRollback>1</enableRollback>
    <enableTimestamp>1</enableTimestamp>
    <vendor>softcrown</vendor>
    <licenseFileList>
        <licenseFile>
            <code>es</code>
            <file>/home/yalilms/Documentos/2ºDAM/Desarrollo_interfaces/AutoCiclo/Instaladores/CarpetaInstalacion/LICENSE.txt
</file>
        </licenseFile>
    </licenseFileList>
    <parameterList>
        <directoryParameter>
            <name>installdir</name>
            <description>Installer.Parameter.installdir.description</description>
            <explanation>Installer.Parameter.installdir.explanation</explanation>
            <value></value>
            <default>${platform_install_prefix}/${product_shortname}-${product_version}</default>
            <allowEmptyValue>0</allowEmptyValue>
            <ask>yes</ask>
            <cliOptionName>prefix</cliOptionName>
            <mustBeWritable>yes</mustBeWritable>
            <mustExist>0</mustExist>
            <width>30</width>
        </directoryParameter>
    </parameterList>
</project>

